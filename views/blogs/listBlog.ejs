 <div class="content">
    <div class="animated fadeIn">
        <div class="row">

            <div class="col-md-12">
                <div class="card">
                    <div class="card-header">
                        <strong class="card-title">Blogs List</strong>
                    </div>
                    <div class="card-body">
                        <table id="bootstrap-data-table" class="table table-striped table-bordered">
                            <thead>
                                <tr>
                                    <th>Sr. No.</th>
                                    <th>Title</th>
                                    <th>Category</th>
                                    <th>Description</th>
                                    <th>Actions</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% if(blogsData.length){
                           
                                for(var i = 0; i< blogsData.length; i++) {%>  
                                <tr>
                                  <td><%= (i+1) %></td>
                                  <td><%= blogsData[i].title%></td>
                                  <td><%= blogsData[i].catId%></td>
                                  <td><%= blogsData[i].description%></td>
                                  <td><a class="btn btn-info" href="/editBlog/<%= blogsData[i].id %>/">Edit</a>
                                    <form class='btn' action='/deleteBlog/<%=blogsData[i].id %>' method='POST'>
                                        <button class='btn btn-danger' >Delete</button>
                                   </form>
                                  </td>
                                </tr>
                                <% }
                                         
                                 }else{ %>
                                     <tr>
                                        <td colspan="3">No user</td>
                                     </tr>
                                  <% } %>  
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

        </div>
    </div><!-- .animated -->